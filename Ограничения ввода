        private void textBox1_KeyPress(object sender, KeyPressEventArgs e)
        {
            if (!Char.IsDigit(e.KeyChar) && e.KeyChar != Convert.ToChar(58) && e.KeyChar != Convert.ToChar(8) && e.KeyChar != Convert.ToChar(45) && e.KeyChar != Convert.ToChar(44))
            {
                e.Handled = true;
            }
        }

        private void textBox2_KeyPress(object sender, KeyPressEventArgs e)
